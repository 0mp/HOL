boolpkg=bool-1.37

HOL4Compatibility.ot.art: HOL4Compatibility.art
	opentheory info --article -o $@ $<

HOL4Compatibility.art: HOL4CompatibilityScript.sml OpenTheoryBoolTheory.uo
	Holmake --ot HOL4CompatibilityTheory.uo

OpenTheoryBoolScript.sml: $(boolpkg).art

$(boolpkg).art:
	opentheory info --theorems -o $@ $(boolpkg)
