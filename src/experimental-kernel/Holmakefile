OPTIONS = NO_OVERLAY
RTAG = $(protect $(SIGOBJ)/RestrictedTag-sig.ui)
HOLSET = $(protect $(SIGOBJ)/HOLset.ui)

Overlay.uo: Overlay.sml RestrictedTerm-sig.uo Definition.ui Net.ui $(RTAG)
	HOLMOSMLC -c -toplevel $(RTAG) RestrictedTerm-sig.ui Definition.ui \
                               Net.ui Overlay.sml

RestrictedTerm-sig.uo: Type.ui $(HOLSET) RestrictedTerm-sig.sml
	HOLMOSMLC -c -toplevel RestrictedTerm-sig.sml

Term.ui: Term.sig RestrictedTerm-sig.uo
	HOLMOSMLC -c RestrictedTerm-sig.ui Term.sig

