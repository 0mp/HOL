OPTIONS = NO_OVERLAY

FTAGD = $(dprot $(SIGOBJ)/FinalTag-sig.ui)
FTAG = $(protect $(SIGOBJ)/FinalTag-sig.ui)

FKINDD = $(dprot $(SIGOBJ)/FinalKind-sig.ui)
FKIND = $(protect $(SIGOBJ)/FinalKind-sig.ui)

FTYPED = $(dprot $(SIGOBJ)/FinalType-sig.ui)
FTYPE = $(protect $(SIGOBJ)/FinalType-sig.ui)

FTERMD = $(dprot $(SIGOBJ)/FinalTerm-sig.ui)
FTERM = $(protect $(SIGOBJ)/FinalTerm-sig.ui)

LIBD = $(dprot $(SIGOBJ)/Lib.ui)


HOLSETD = $(dprot $(SIGOBJ)/HOLset.ui)

Overlay.uo: Overlay.sml Definition.ui Net.ui $(FTAGD) $(FKINDD) $(FTYPED) $(FTERMD)
	$(HOLMOSMLC) -c -toplevel $(FTAG) $(FKIND) $(FTYPE) $(FTERM) Definition.ui \
                                  Net.ui Overlay.sml

Kind.ui : Kind.sig $(FKINDD) $(dprot $(SIGOBJ)/KernelSig.ui) $(LIBD)
	$(HOLMOSMLC) -c $(FKIND) Kind.sig

Type.ui : Type.sig $(FTYPED) Kind.ui
	$(HOLMOSMLC) -c $(FTYPE) Type.sig

Term.ui: Term.sig $(FTERMD) Type.ui
	$(HOLMOSMLC) -c $(FTERM) Term.sig

