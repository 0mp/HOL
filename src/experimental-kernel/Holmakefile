OPTIONS = NO_OVERLAY

FTAGD = $(dprot $(SIGOBJ)/FinalTag-sig.ui)
FTAG = $(protect $(SIGOBJ)/FinalTag-sig.ui)

FTYPED = $(dprot $(SIGOBJ)/FinalType-sig.ui)
FTYPE = $(protect $(SIGOBJ)/FinalType-sig.ui)

FTERMD = $(dprot $(SIGOBJ)/FinalTerm-sig.ui)
FTERM = $(protect $(SIGOBJ)/FinalTerm-sig.ui)

FTHEORYD = $(dprot $(SIGOBJ)/FinalTheory-sig.ui)
FTHEORY = $(protect $(SIGOBJ)/FinalTheory-sig.ui)

LIBD = $(dprot $(SIGOBJ)/Lib.ui)

HOLSETD = $(dprot $(SIGOBJ)/HOLset.ui)

Overlay.uo: Overlay.sml Definition.ui Net.ui $(FTAGD) $(FTYPED) $(FTERMD) $(BASISD)
	$(HOLMOSMLC) -c -toplevel $(FTAG) $(FTYPE) $(FTERM) Definition.ui \
                                  Net.ui Overlay.sml

Type.ui : Type.sig $(FTYPED) $(dprot $(SIGOBJ)/KernelSig.ui) $(LIBD)
	$(HOLMOSMLC) -c $(FTYPE) Type.sig

Term.ui: Term.sig $(FTERMD) Type.ui
	$(HOLMOSMLC) -c $(FTERM) Term.sig

Theory.ui: Theory.sig $(FTHEORYD) Term.ui Type.ui Thm.ui
	$(HOLMOSMLC) -c $(FTHEORY) Theory.sig