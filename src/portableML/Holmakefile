OPTIONS = NO_OVERLAY

UOFILES = $(patsubst %.sml,%.uo,$(wildcard *.sml))

FRS = FunctionalRecordUpdate

all: $(UOFILES) selftest.exe $(FRS).uo

selftest.exe: selftest.uo Redblackset.uo
	$(MOSMLC) -o $@ $<

$(FRS).sml: $(dprot $(HOLDIR)/tools-poly/Holmake/$(FRS).sml)
	$(CP) $< $@

EXTRA_CLEANS = $(FRS).sml
