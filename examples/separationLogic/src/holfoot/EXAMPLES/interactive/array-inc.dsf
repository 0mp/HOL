inc(;a,n) [data_array(a,n,data)] {
   local i, tmp;
   i = 0;
   loop_spec [data_array(a,n,data) * (i == #ic)] {
      while (i < n) {
         tmp = (a + i) -> dta;
         (a + i) -> dta = tmp + 1;
         i = i + 1;
      }
   } [data_array(a, n, _data2) *
      ``(!id. id < #ic ==> 
              (EL id data2 = EL id data)) /\
        (!id. #ic <= id /\ id < n ==>
              (EL id data2 = SUC (EL id data)))``]               
} [data_array(a,n,``MAP SUC data``)]