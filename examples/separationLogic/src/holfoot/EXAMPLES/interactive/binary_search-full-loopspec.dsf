binsearch(f;a,n,e) [data_array(a,n,data) * ``SORTED $<= data``] {
   local l, r, m, tmp;

   l = 0;
   r = n;
   f = 0;
   loop_spec [data_array(a+l,r-l,data) * (f == #fc) *
              (l == #lc) * (r == #rc) * 
              ``IS_BOOL_TO_NUM #fc /\ SORTED $<= data /\
                (LENGTH data = rc - lc)``] {
   while ((f == 0) and (l < r)) {
      block_spec [l < r] {
         m = l + ((r - l) / 2);
      } [l <= m * m < r]
      tmp = (a+m)->dta;
      if (tmp < e) {
         l = m+1;
      } else if (e < tmp) {
         r = m;
      } else {
         f = 1;
      }
   }} [data_array(a+#lc,#rc-#lc,data) *
      (f == ``BOOL_TO_NUM (MEM e data \/ (#fc = 1))``)]
} [data_array(a,n,data) * (f == ``BOOL_TO_NUM (MEM e data)`` * ``SORTED $<= data``)]
