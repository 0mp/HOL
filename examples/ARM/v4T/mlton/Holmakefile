EXTRA_CLEANS = run paths mosrun.exe run.in run.out
OPTIONS = NO_OVERLAY
INCLUDES=$(HOLDIR)/src/theoryML

all: run mosrun.exe

paths:
	@echo "MLYACC" $(HOLDIR)/tools/mlyacc/mlyacclib > paths
	@echo "MLTON" $(HOLDIR)/tools/mlton/pre >> paths
	@echo "PORTABLE" $(HOLDIR)/src/portableML >> paths
	@echo "INCLUDES" $(INCLUDES) >> paths

run: run.mlb paths evalML.sml
	mlton -verbose 1 -mlb-path-map paths run.mlb

mosrun.exe: evalML.sml
	HOLMOSMLC -toplevel -I $(INCLUDES) -I .. -o $@ evalML.sml

ppstream.uo:
	@
