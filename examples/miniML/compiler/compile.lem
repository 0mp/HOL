(* Semantics *)

let rec
Cv_to_ov m (CLit l) = OLit l
and
Cv_to_ov m (CConv cn vs) = OConv (Pmap.find cn m) (List.map (Cv_to_ov m) vs)
and
Cv_to_ov m (CClosure _ _ _) = OFn
and
Cv_to_ov m (CRecClos _ _ _ _) = OFn

let sort_Cenv Cenv = qsort a_linear_order Cenv

let rec
doPrim2 b ty op (CLit (IntLit x)) (CLit (IntLit y)) =
  if b && y = i0 then CRaise Div_error
  else CVal (CLit (ty (op x y)))

let rec
CevalPrim2 CAdd = doPrim2 false IntLit Int.(+)
and
CevalPrim2 CSub = doPrim2 false IntLit Int.(-)
and
CevalPrim2 CMul = doPrim2 false IntLit Int.( *)
and
CevalPrim2 CDiv = doPrim2 true IntLit Int.(/)
and
CevalPrim2 CMod = doPrim2 true IntLit Int.(%)
and
CevalPrim2 CLt = doPrim2 false Bool Int.(<)
and
CevalPrim2 CEq = doPrim2 false Bool (=)

let rec
Cpmatch env (CPvar n) v = Some (Pmap.add n v env)
and
Cpmatch env (CPlit l) (CLit l') =
  if l = l' then Some env else None
and
Cpmatch env (CPcon n ps) (CConv n' vs) =
  if n = n' then Cpmatch_list env ps vs else None
and
Cpmatch_list env [] [] = Some env
and
Cpmatch_list env (p::ps) (v::vs) =
  match Cpmatch env p v with
  | None -> None
  | Some env' -> Cpmatch_list env' ps vs
  end

indreln
forall env error.
true
==>
Cevaluate env (CRaise error) (Rerr (Rraise error))

and
forall env n v.
(Pmap.find n env = v) (* TODO: parens :( *)
==>
Cevaluate env (CVar n) (Rval v)

and
forall env v.
true
==>
Cevaluate env (CVal v) (Rval v)

and
forall env n es vs.
Cevaluate_list env es (Rval vs)
==>
Cevaluate env (CCon n es) (Rval (CConv n vs))
and
forall env n es err.
Cevaluate_list env es (Rerr err)
==>
Cevaluate env (CCon n es) (Rerr err)

and
forall env e n m vs.
Cevaluate env e (Rval (CConv m vs))
==>
Cevaluate env (CTagEq e n) (Rval (CLit (Bool (n = m))))
and
forall env e n err.
Cevaluate env e (Rerr err)
==>
Cevaluate env (CTagEq e n) (Rerr err)

and
forall env e n m vs.
Cevaluate env e (Rval (CConv m vs))
==>
Cevaluate env (CProj e n) (Rval (List.nth vs n))
and
forall env e n err.
Cevaluate env e (Rerr err)
==>
Cevaluate env (CProj e n) (Rerr err)

and
forall env n.
true
==>
Cevaluate env (CMat n []) (Rerr (Rraise Bind_error))
and
forall env n p e pes env' r.
Cpmatch env p (Pmap.find n env) = Some env' &&
Cevaluate env' e r
==>
Cevaluate env (CMat n ((p,e)::pes)) r
and
forall env n p e pes r.
Cpmatch env p (Pmap.find n env) = None &&
Cevaluate env (CMat n pes) r
==>
Cevaluate env (CMat n ((p,e)::pes)) r

and
forall env b r.
Cevaluate env b r
==>
Cevaluate env (CLet [] [] b) r
and
forall env n ns e es b v r.
Cevaluate env e (Rval v) &&
Cevaluate (Pmap.add n v env) (CLet ns es b) r
==>
Cevaluate env (CLet (n::ns) (e::es) b) r

and
forall env ns defs b r.
Cevaluate
  (fold_left2
    (fun env' n (ns,b) ->
      Pmap.add n (CClosure (sort_Cenv (fmap_to_alist env)) ns b) env')
    env ns defs)
  b r
==>
Cevaluate env (CLetfun false ns defs b) r

and
forall env ns defs b r.
Cevaluate
  (List.fold_left
     (fun env' n ->
       Pmap.add n (CRecClos (sort_Cenv (fmap_to_alist env)) ns defs n) env')
     env ns)
  b r
==>
Cevaluate env (CLetfun true ns defs b) r

and
forall env ns b.
true
==>
Cevaluate env (CFun ns b) (Rval (CClosure (sort_Cenv (fmap_to_alist env)) ns b))

and
forall env e es env' ns b vs r.
Cevaluate env e (Rval (CClosure env' ns b)) &&
Cevaluate_list env es (Rval vs) &&
Cevaluate (fold_left2 (fun en n v -> Pmap.add n v en)
             (alist_to_fmap env') ns vs) b r
==>
Cevaluate env (CCall e es) r
and
forall env e es env' ns b err.
Cevaluate env e (Rval (CClosure env' ns b)) &&
Cevaluate_list env es (Rerr err)
==>
Cevaluate env (CCall e es) (Rerr err)

and
forall env e es env' ns' defs n i ns b vs r.
Cevaluate env e (Rval (CRecClos env' ns' defs n)) &&
Cevaluate_list env es (Rval vs) &&
find_index n ns' 0 = Some i &&
List.nth defs i = (ns,b) &&
Cevaluate
  (fold_left2 (fun en n v -> Pmap.add n v en)
    (snd (fold_left2
            (fun (n,en) n' def ->
              (n+1, Pmap.add n' (CRecClos env' ns' defs n) en))
            (0,alist_to_fmap env')
            ns' defs))
    ns vs)
  b r
==>
Cevaluate env (CCall e es) r
and
forall env e es env' ns' defs n err.
Cevaluate env e (Rval (CRecClos env' ns' defs n)) &&
Cevaluate_list env es (Rerr err)
==>
Cevaluate env (CCall e es) (Rerr err)

and
forall env e es err.
Cevaluate env e (Rerr err)
==>
Cevaluate env (CCall e es) (Rerr err)

and
forall env p2 e1 e2 v1 v2 e3 r.
Cevaluate_list env [e1;e2] (Rval [v1;v2]) &&
CevalPrim2 p2 v1 v2 = e3 &&
Cevaluate env e3 r
==>
Cevaluate env (CPrim2 p2 e1 e2) r
and
forall env p2 e1 e2 err.
Cevaluate_list env [e1;e2] (Rerr err)
==>
Cevaluate env (CPrim2 p2 e1 e2) (Rerr err)

and
forall env es v1 v2 e3 r.
Cevaluate_list env es (Rval [v1;v2]) &&
doPrim2 false Bool Int.(<=) v1 v2 = e3 &&
Cevaluate env e3 r
==>
Cevaluate env (CLprim CLeq es) r
and
forall env es err.
Cevaluate_list env es (Rerr err)
==>
Cevaluate env (CLprim CLeq es) (Rerr err)

and
forall env e1 e2 e3 b1 r.
Cevaluate env e1 (Rval (CLit (Bool b1))) &&
Cevaluate env (if b1 then e2 else e3) r
==>
Cevaluate env (CLprim CIf [e1;e2;e3]) r
and
forall env e1 e2 e3 err.
Cevaluate env e1 (Rerr err)
==>
Cevaluate env (CLprim CIf [e1;e2;e3]) (Rerr err)

and
forall env e1 e2.
Cevaluate env e1 (Rval (CLit (Bool false)))
==>
Cevaluate env (CLprim CAnd [e1;e2]) (Rval (CLit (Bool false)))
and
forall env e1 e2 r.
Cevaluate env e1 (Rval (CLit (Bool true))) &&
Cevaluate env e2 r
==>
Cevaluate env (CLprim CAnd [e1;e2]) r
and
forall env e1 e2 err.
Cevaluate env e1 (Rerr err)
==>
Cevaluate env (CLprim CAnd [e1;e2]) (Rerr err)

and
forall env e1 e2.
Cevaluate env e1 (Rval (CLit (Bool true)))
==>
Cevaluate env (CLprim COr [e1;e2]) (Rval (CLit (Bool true)))
and
forall env e1 e2 r.
Cevaluate env e1 (Rval (CLit (Bool false))) &&
Cevaluate env e2 r
==>
Cevaluate env (CLprim COr [e1;e2]) r
and
forall env e1 e2 err.
Cevaluate env e1 (Rerr err)
==>
Cevaluate env (CLprim COr [e1;e2]) (Rerr err)

and
forall env.
true
==>
Cevaluate_list env [] (Rval [])
and
forall env e es v vs.
Cevaluate env e (Rval v) &&
Cevaluate_list env es (Rval vs)
==>
Cevaluate_list env (e::es) (Rval (v::vs))
and
forall env e es err.
Cevaluate env e (Rerr err)
==>
Cevaluate_list env (e::es) (Rerr err)
and
forall env e es v err.
Cevaluate env e (Rval v) &&
Cevaluate_list env es (Rerr err)
==>
Cevaluate_list env (e::es) (Rerr err)

(* relating source to intermediate language *)

indreln
forall G cm env Cenv err.
true
==>
exp_Cexp G cm env Cenv (Raise err) (CRaise err)
and
forall G cm env Cenv v Cv.
G cm v Cv
==>
exp_Cexp G cm env Cenv (Val v) (CVal Cv)
and
forall G cm env Cenv cn es Ces.
Pmap.mem cn cm &&
every2 (exp_Cexp G cm env Cenv) es Ces
==>
exp_Cexp G cm env Cenv (Con cn es) (CCon (Pmap.find cn cm) Ces)
and
forall G cm env Cenv vn v Cvn.
lookup vn env = Some v &&
Pmap.mem Cvn Cenv &&
G cm v (Pmap.find Cvn Cenv)
==>
exp_Cexp G cm env Cenv (Var vn) (CVar Cvn)
and
forall G cm env Cenv vn e n Ce.
(forall v Cv. G cm v Cv -->
  exp_Cexp G cm (bind vn v env) (Pmap.add n Cv Cenv) e Ce)
==>
exp_Cexp G cm env Cenv (Fun vn e) (CFun [n] Ce)

indreln
forall G cm l.
true
==>
v_Cv G cm (Lit l) (CLit l)
and
forall G cm cn vs Cvs.
Pmap.mem cn cm &&
every2 (v_Cv G cm) vs Cvs
==>
v_Cv G cm (Conv cn vs) (CConv (Pmap.find cn cm) Cvs)
and
forall G cm env vn e Cenv n Ce.
(forall v Cv. G cm v Cv -->
  exp_Cexp G cm (bind vn v env) (Pmap.add n Cv (alist_to_fmap Cenv)) e Ce)
==>
v_Cv G cm (Closure env vn e) (CClosure Cenv [n] Ce)

