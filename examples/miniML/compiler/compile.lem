let least_not_in s = least (fun n -> not (n IN s))

type exp_to_Cexp_state =
   (* bindings of variable names to numbers *)
  <| m : (string,num) Pmap.map
   (* next unused temporary variable number *)
   ; n : num
   (* not yet bound (in this expression) declaration variables *)
   ; ds : string set
   (* (outermost, or previous) bindings of declaration variables *)
   ; dm : (string,num) Pmap.map
   (* next unused declaration variable number *)
   ; dn : num
   |>

let extend tp s vn =
  if tp && vn IN s.ds then
    if Pmap.mem vn s.dm then
      let n = Pmap.find vn s.dm in
      (<|s with ds = s.ds \ {vn}; m = Pmap.add vn n s.m|>, n)
    else
      (<|s with ds = s.ds \ {vn}; m = Pmap.add vn s.dn s.m;
         dm = Pmap.add vn s.dn s.dm; dn = s.dn+1|>, s.dn)
  else
    (<|s with m = Pmap.add vn s.n s.m; n = s.n+1|>, s.n)

let rec
pat_to_Cpat tp cm (s, Pvar vn) =
  let (s',n) = extend tp s vn in (s', CPvar n)
and
pat_to_Cpat tp cm (s, Plit l) = (s, CPlit l)
and
pat_to_Cpat tp cm (s, Pcon cn ps) =
  let (s',ps) = List.fold_right
    (fun p (s,ps) -> let (s',p) = pat_to_Cpat tp cm (s,p) in (s',p::ps))
         ps (s,[]) in
  (s', CPcon (Pmap.find cn cm) ps)

let rec
exp_to_Cexp tp cm (s, Raise err) = (s, CRaise err)
and
exp_to_Cexp tp cm (s, Val (Lit l)) = (s, CVal (CLit l))
and
exp_to_Cexp tp cm (s, Con cn es) =
  (s, CCon (Pmap.find cn cm) (List.map (fun e -> let (_s,e) = exp_to_Cexp false cm (s,e) in e) es))
and
exp_to_Cexp tp cm (s, Var vn) = (s, CVar (Pmap.find vn s.m))
and
exp_to_Cexp tp cm (s, Fun vn e) =
  let (s',n) = extend false s vn in
  let (_s,Ce) = exp_to_Cexp false cm (s', e) in
  (s, CFun [n] Ce)
and
exp_to_Cexp tp cm (s, App (Opn opn) e1 e2) =
  let (_s,Ce1) = exp_to_Cexp false cm (s, e1) in
  let (_s,Ce2) = exp_to_Cexp false cm (s, e2) in
  (s, CPrim2 (match opn with
              | Plus   -> CAdd
              | Minus  -> CSub
              | Times  -> CMul
              | Divide -> CDiv
              | Modulo -> CMod
              end)
      Ce1 Ce2)
and
exp_to_Cexp tp cm (s, App (Opb opb) e1 e2) =
  let (_s,Ce1) = exp_to_Cexp false cm (s, e1) in
  let (_s,Ce2) = exp_to_Cexp false cm (s, e2) in
  (s, match opb with
      | Lt -> CPrim2 CLt Ce1 Ce2
      | Gt -> CPrim2 CLt Ce2 Ce1
      | Leq -> CLprim CLeq [Ce1;Ce2]
      | Geq -> CLprim CLeq [Ce2;Ce1]
      end)
and
exp_to_Cexp tp cm (s, App Equality e1 e2) =
  let (_s,Ce1) = exp_to_Cexp false cm (s, e1) in
  let (_s,Ce2) = exp_to_Cexp false cm (s, e2) in
  (s, CPrim2 CEq Ce1 Ce2)
and
exp_to_Cexp tp cm (s, App Opapp e1 e2) =
  let (_s,Ce1) = exp_to_Cexp false cm (s, e1) in
  let (_s,Ce2) = exp_to_Cexp false cm (s, e2) in
  (s, CCall Ce1 [Ce2])
and
exp_to_Cexp tp cm (s, Log log e1 e2) =
  let (_s,Ce1) = exp_to_Cexp false cm (s, e1) in
  let (_s,Ce2) = exp_to_Cexp false cm (s, e2) in
  (s, CLprim (match log with
              | And -> CAnd
              | Or  -> COr
              end)
      [Ce1;Ce2])
and
exp_to_Cexp tp cm (s, If e1 e2 e3) =
  let (_s,Ce1) = exp_to_Cexp false cm (s, e1) in
  let (s',Ce2) = exp_to_Cexp tp cm (s, e2) in
  let (s',Ce3) = exp_to_Cexp tp cm (s', e3) in
  (if tp then s' else s, CLprim CIf [Ce1;Ce2;Ce3])
and
exp_to_Cexp tp cm (s, Mat (Var vn) pes) =
  let (s',Cpes) = List.fold_right
    (fun (p,e) (s,Cpes) ->
      let (s,Cp) = pat_to_Cpat tp cm (s,p) in
      let (s,Ce) = exp_to_Cexp tp cm (s,e) in
      (s,(Cp,Ce)::Cpes))
          pes  (s,[]) in
  (if tp then s' else s, CMat (Pmap.find vn s.m) Cpes)
and
exp_to_Cexp tp cm (s, Mat e pes) =
  let (_s,Ce) = exp_to_Cexp false cm (s, e) in
  let (s',n) = (<|s with n = s.n+1|>, s.n) in
  let (s',Cpes) = List.fold_right
    (fun (p,e) (s,Cpes) ->
      let (s,Cp) = pat_to_Cpat tp cm (s,p) in
      let (s,Ce) = exp_to_Cexp tp cm (s,e) in
      (s,(Cp,Ce)::Cpes))
          pes  (s',[]) in
  (if tp then s' else s, CLet [n] [Ce] (CMat n Cpes))
and
exp_to_Cexp tp cm (s, Let vn e b) =
  let (s',n) = extend tp s vn in
  let (_s,Ce) = exp_to_Cexp false cm (s, e) in
  let (s',Cb) = exp_to_Cexp tp cm (s', b) in
  (if tp then s' else s, CLet [n] [Ce] Cb)
and
exp_to_Cexp tp cm (s, Letrec defs b) =
  let (s',fns) = List.fold_right
    (fun (d,_vn,_e) (s,fns) -> let (s,n) = extend tp s d in (s, n::fns))
          defs      (s,[]) in
  let Cdefs = List.fold_right
    (fun (_d,vn,e) Cdefs ->
      let (s',n) = extend false s' vn in
      let (_s,Ce) = exp_to_Cexp false cm (s', e) in
      ([n],Ce)::Cdefs)
          defs     [] in
  let (s',Cb) = exp_to_Cexp tp cm (s',b) in
  (if tp then s' else s, CLetfun true fns Cdefs Cb)

let rec
v_to_Cv cm (s, Lit l) = CLit l
and
v_to_Cv cm (s, Conv cn vs) =
  CConv (Pmap.find cn cm) (List.map (fun v -> v_to_Cv cm (s,v)) vs)
and
v_to_Cv cm (s, Closure env vn e) =
  let Cenv = List.map (fun (x,v) -> (Pmap.find x s.m, v_to_Cv cm (s,v))) env in
  let (s',n) = extend false s vn in
  let (_s,Ce) = exp_to_Cexp false cm (s', e) in
  CClosure (sort_Cenv Cenv) [n] Ce
and
v_to_Cv cm (s, Recclosure env defs vn) =
  let Cenv = List.map (fun (x,v) -> (Pmap.find x s.m, v_to_Cv cm (s,v))) env in
  let (s',fns) = List.fold_right
    (fun (d,_vn,_e) (s,fns) -> let (s,n) = extend false s d in (s, n::fns))
          defs      (s,[]) in
  let Cdefs = List.fold_right
    (fun (_d,vn,e) Cdefs ->
      let (s',n) = extend false s' vn in
      let (_s,Ce) = exp_to_Cexp false cm (s', e) in
      ([n],Ce)::Cdefs)
          defs     [] in
  CRecClos (sort_Cenv Cenv) fns Cdefs (Pmap.find vn s.m)

