INCLUDES=$(HOLDIR)/examples/countable .. ../semantics ../metatheory ./bytecode
OPTIONS = QUIT_ON_FAILURE

ifdef POLY
HOLHEAP = miniml-heap
EXTRA_CLEANS = $(HOLHEAP) $(HOLHEAP).o

BARE_THYS = $(HOLDIR)/examples/countable/countableTheory ../semantics/MiniMLTerminationTheory ../metatheory/evaluateEquationsTheory ./bytecode/BytecodeTheory
DEPS = $(patsubst %,%.uo,$(BARE_THYS))

$(HOLHEAP): $(DEPS)
	$(protect $(HOLDIR)/bin/buildheap) -o $(HOLHEAP) $(BARE_THYS)
endif
