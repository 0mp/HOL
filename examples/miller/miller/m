export MPATH="$MPATH -> miller"

if test "$1" = "clean"
then
        echo -n "$MPATH: cleaning... "
        Holmake cleanAll
        echo "done."
        exit 0
fi

echo -n "$MPATH: building... "
Holmake -I ../ho_prover -I ../subtypes -I ../RSA -I ../formalize -I ../prob -I ../groups --qof
echo "done."
