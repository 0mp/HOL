${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code/instruction-set-models/arm
echo ${HOLDIR}/examples/machine-code/instruction-set-models/arm
${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code/instruction-set-models/x86
echo ${HOLDIR}/examples/machine-code/instruction-set-models/x86
${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code/instruction-set-models/x86_64
echo ${HOLDIR}/examples/machine-code/instruction-set-models/x86_64
${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code/instruction-set-models/ppc
echo ${HOLDIR}/examples/machine-code/instruction-set-models/ppc
${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code/decompiler
echo ${HOLDIR}/examples/machine-code/decompiler
${HOLDIR}/bin/Holmake cleanAll
