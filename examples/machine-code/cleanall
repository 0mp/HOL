echo ${HOLDIR}/examples/machine-code/garbage-collectors
${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code/just-in-time
echo ${HOLDIR}/examples/machine-code/just-in-time
${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code
