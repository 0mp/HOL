echo ${HOLDIR}/examples/machine-code/compiler
${HOLDIR}/bin/Holmake cleanAll
cd ${HOLDIR}/examples/machine-code/hoare-triple
echo ${HOLDIR}/examples/machine-code/hoare-triple
${HOLDIR}/bin/Holmake cleanAll
