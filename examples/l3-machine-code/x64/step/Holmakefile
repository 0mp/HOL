INCLUDES = ../model
OPTIONS = QUIT_ON_FAILURE

ifdef POLY
HOLHEAP = x64-heap
EXTRA_CLEANS = $(HOLHEAP) $(HOLHEAP).o

BARE_THYS = ../model/x64Lib ../model/x64AssemblerLib
DEPS = $(patsubst %,%.uo,$(BARE_THYS))

$(HOLHEAP): $(DEPS)
	$(protect $(HOLDIR)/bin/buildheap) -b ../../base/base-heap -o $(HOLHEAP) $(BARE_THYS)

endif
