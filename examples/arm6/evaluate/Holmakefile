INCLUDES = ..
EXTRA_CLEANS = Data.ui Data.uo Lexer.sml Parser.sig Parser.sml

Data.uo : Data.sml
	HOLMOSMLC -c Data.sml

Parser.sml : Parser.grm Data.uo
	mosmlyac Parser.grm

Lexer.sml : Lexer.lex Parser.sml Data.uo
	mosmllex Lexer.lex

Parser.ui Parser.uo Lexer.ui Lexer.uo: Parser.sml Lexer.sml
	HOLMOSMLC -c Parser.sig Parser.sml Lexer.sml
