CLINE_OPTIONS=-j1

all: LassieTestTheory.sml
.PHONY: all

interactive/lassie.lexicon:
	export LASSIEDIR=../ &&\
	./init.sh &&\
	cd ../sempre &&\
	ant core interactive &&\
	cd ../src

LassieTestTheory.sml: interactive/lassie.lexicon

sempre_clean: $(clean)
	cd ../sempre &&\
	rm -rf classes/ fig/ int-output/ interactive/lassie.lexicon interactive/sempre-out-socket.sml lib/ libsempre/ module-classes.txt state/
.PHONY: sempre_clean
