EXTRA_CLEANS = assemblerML.ui assemblerML.uo Data.ui Data.uo\
	Lexer.sml Parser.sig Parser.sml
INCLUDES = /local/scratch/acjf3/hol98/src/theoryML

Data.uo : Data.sml
	HOLMOSMLC -c Data.sml

Parser.sml : Parser.grm Data.uo
	mosmlyac Parser.grm

Lexer.sml : Lexer.lex Parser.sml Data.uo
	mosmllex Lexer.lex

Parser.ui Parser.uo Lexer.ui Lexer.uo: Parser.sml Lexer.sml
	HOLMOSMLC -c Parser.sig Parser.sml Lexer.sml

assemblerML.uo: Parser.sml Lexer.sml assemblerML.sml assemblerML.sig
	HOLMOSMLC -c assemblerML.sml
