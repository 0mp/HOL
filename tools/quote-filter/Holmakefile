
quote-filter: quote-filter.uo filter.uo
	MOSMLC -o quote-filter quote-filter.uo

quote-filter.uo: quote-filter.sml filter.uo
	MOSMLC -c quote-filter.sml

filter.sml : filter.lex
	MOSMLLEX filter.lex

filter.uo: filter.sml
	MOSMLC -c filter.sml

Clean:
	/bin/rm -f filter.{uo,ui,sml} quote-filter.{uo,ui} quote-filter
