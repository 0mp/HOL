OPTIONS = NO_OVERLAY
EXTRA_CLEANS = filter.sml quote-filter selftest

all: quote-filter selftest

quote-filter: quote-filter.uo filter.uo
	MOSMLC -o quote-filter quote-filter.uo

filter.sml : filter
	$(protect $(HOLDIR)/tools/mllex/mllex.exe) filter

selftest: selftest.sml
	MOSMLC -o selftest selftest.sml
